{
  "name": "whiterussianstudio/kat-kit",
  "description": "Whiter Russian Kirby CMS, Alpinejs, TailwindCSS Kit",
  "type": "project",
  "require": {
    "php": ">=7.4.0 <8.2.0",
    "getkirby/cms": "^3.5",
    "arnoson/kirby-vite": "^2.4",
    "bnomei/kirby3-php-cachedriver": "^1.3"
  },
  "extra": {
    "kirby-plugin-path": "app/plugins"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  }
}
